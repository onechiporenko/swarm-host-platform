{
  "name": "swarm-host-cli",
  "version": "1.1.0",
  "description": "CLI for Swarm-Host",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && rm -rf dist/tests* && find ./dist -name \"*.js.map\" -type f -delete && cp -r dist/lib/* dist/. && rm -rf dist/lib",
    "test": "mocha -r ts-node/register \"tests/**/*.ts\"",
    "lint": "tslint -c tslint.json lib/**/*.ts && tslint -c tests/tslint.json tests/**/*.ts"
  },
  "keywords": [
    "cli",
    "swarm-host"
  ],
  "author": "onechiporenko",
  "license": "MIT",
  "bin": {
    "swarm-host": "./dist/index.js"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "shelljs": "^0.8.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/shelljs": "^0.8.0",
    "@types/sinon": "^4.3.0",
    "@types/yargs": "^10.0.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "execa": "^0.10.0",
    "jest-cli": "^22.4.3",
    "mocha": "^4.1.0",
    "sinon": "^4.4.8",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
